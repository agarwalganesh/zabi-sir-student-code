#include <stdio.h>

int main() {

    int n ;
    scanf("%d",&n);
    int count =1;
    
    
    for(int i =1;i<=n;++i){
         int num = count;

        for(int j =1;j<=i;++j){
            
           printf("%d ",num--);
                
        }
    printf("\n");
         count = count + (i+1);
    }
    return 0;
}
  
