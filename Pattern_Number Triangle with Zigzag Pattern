#include <stdio.h>

int main() {
int n;
    scanf("%d",&n);
    
    for(int i=1; i<=n ; ++i){
        
                    
        int a=i;
        for(int j=1 ; j<=i;++j){
            printf("%d ",a);
          if(j%2!=0){
                a = a + 2*(n-i+1)-1;
                // printf("%d ",a);
            }
            else {
                a= a + 2*(i-j);
                // printf("%d ",a);
            }
           
        }
        printf("\n");
    }
    return 0;
}



