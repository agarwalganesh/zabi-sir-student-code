

#include <stdio.h>

int main() {

long long n;
    scanf("%lld",&n);
    
    
  if (n==1) { 
        printf("1");
      return 0;
    }
    


    long long int i =1;
    long long int product = 1;
    for(i=1; product<n ;++i ){
        product= product * i; 
    }
    
    
    if(product==n){
        printf("%lld",i-1);
    }
    else{
        printf("NO");
    }
    
    return 0;
}
    
