#include <stdio.h>

int main() {
    int n, sum = 0, temp;
    scanf("%d", &n);
    
    temp = n;
    
    while (temp > 0) {
        sum += temp % 10;
        temp /= 10;
    }
    
    if (n % sum == 0) {
        printf("%d is a harshad number", n);
    } else {
        printf("%d is not a harshad number", n);
    }
    
    return 0;
}
