
#include <stdio.h>

int main() {
int n;
    scanf("%d", &n);
    n= (n<<4)-n-n-n-n-n-n-n-n-n;
    printf("%d", n);
    return 0;
}




#include <stdio.h>

int main() {
int n;
    scanf("%d", &n);
    n= (n<<3)-n;
    printf("%d", n);
    return 0;
}


#include <stdio.h>

int main() {
int n;
    scanf("%d", &n);
    n= (n<<2)+n +n +n;
    printf("%d", n);
    return 0;
}
