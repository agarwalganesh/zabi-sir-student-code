#include <stdio.h>

int main() {

    int n;
    scanf("%d",&n);
    
    for(int i=1; i<=n ; ++i){
        for(int j=0; j<=i-1 ; ++j){
          
            if(i%2!=0){
            
                printf("%d ",j*2+1);
            }else{
                printf("%d ",j*2+2);
            }
            
        }
        printf("\n");
    }
    return 0;
}    
